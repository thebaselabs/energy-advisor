{
  "version": 2,
  "builds": [
    {
      "src": "api/analyze.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "/api/analyze.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
